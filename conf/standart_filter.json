{
  "description": "Standart filter. You can modify it.",
  "processors": [
    {
      "rename": {
        "field": "message",
        "target_field": "incoming_message"
      }
    },
    {
      "gsub": {
        "field": "incoming_message",
        "pattern": "<\\d+>",
        "replacement": ""
      }
    },
    {
      "kv": {
        "field": "incoming_message",
        "field_split": "\\|",
        "value_split": "="
      }
    },
    {
      "remove": {
        "field": "incoming_message"
      }
    }
  ]
}
